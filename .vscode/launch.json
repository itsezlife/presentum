{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run App (Debug)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}/example",
      "args": [
        "--dart-define-from-file=config/development.json",
        "--dart-define=octopus.logs=true",
        "--dart-define=octopus.measure=false",
        "--dart-define=presentum.logs=true"
      ],
      "env": {}
    },
    {
      "name": "Web server (Debug)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}/example",
      "args": [
        "--dart-define-from-file=config/development.json",
        "--device-id=web-server",
        "--dart-define=octopus.logs=true",
        "--dart-define=octopus.measure=false",
        "--dart-define=presentum.logs=true"
      ],
      "env": {}
    },
    {
      "name": "Integration tests (Debug)",
      "type": "dart",
      "program": "${workspaceFolder}/example/integration_test/app_test.dart",
      "request": "launch",
      "cwd": "${workspaceFolder}/example",
      "args": ["--dart-define-from-file=config/development.json"],
      "env": {}
    }
  ]
}
